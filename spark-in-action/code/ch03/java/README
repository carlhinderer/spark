# Build the project
$ mvn clean install


# Run the IngestionSchemaManipulationApp (Uses custom 'mainClass' property)
$ mvn exec:exec -DmainClass=lab200_ingestion_schema_manipulation.IngestionSchemaManipulationApp

# Run the SchemaIntrospectionApp
$ mvn exec:exec -DmainClass=lab210_schema_introspection.SchemaIntrospectionApp

# Run the JsonIngestionSchemaManipulationApp
$ mvn exec:exec -DmainClass=lab220_json_ingestion_schema_manipulation.JsonIngestionSchemaManipulationApp

# Run the DataFrameUnionApp
$ mvn exec:exec -DmainClass=lab230_dataframe_union.DataframeUnionApp


# Run the ArrayToDatasetApp
$ mvn exec:exec -DmainClass=lab300_dataset.ArrayToDatasetApp
